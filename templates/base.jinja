    <body>
         <div class="hd">
            <!-- Links -->
            <a href="{{ url_for('login') }}" style="background-color:#97a1d0;">Login</a>
            <a href="{{ url_for('signup') }}" style="background-color:#9dc0e1;">Signup</a>
            {% if session['logged_in'] == True %}
                <a href="{{ url_for('repo', username=username) }}" style="background-color:#f8da87;">Repo</a>
